class GaussianERFFitException(Exception):
    def __int__(self,name): 
        self.log_file_name = "log_msg.txt"
        self.log_file = open(self.log_file_name, "w")
        
    def write_log_msg(self,message):                
        self.log_file.write("warning: {0}".format(message))

if __name__ == "__main__" :
    t = GaussianERFFitException()
    t.write_log_msg("read_dose.exe")
    t.write_log_msg("test.exe")